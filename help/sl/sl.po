msgstr "Nova igra"

#: C/shortcuts.page:21(td/p)
msgid "<keyseq><key>Ctrl</key><key>N</key></keyseq>"
msgstr "<keyseq><key>Ctrl</key><key>N</key></keyseq>"

#: C/shortcuts.page:24(td/p)
msgid "Fullscreen"
msgstr "Celozaslonski način"

#: C/shortcuts.page:25(td/p)
msgid "<key>F11</key>"
msgstr "<key>F11</key>"

#: C/shortcuts.page:28(td/p)
msgid "Hint"
msgstr "Namig"

#: C/shortcuts.page:29(td/p)
msgid "<keyseq><key>Ctrl</key><key>H</key></keyseq>"
msgstr "<keyseq><key>Ctrl</key><key>H</key></keyseq>"

#: C/shortcuts.page:32(td/p)
msgid "Pause"
msgstr "Premor"

#: C/shortcuts.page:33(td/p)
msgid "<key>Pause</key>"
msgstr "<key>Pause</key>"

#: C/shortcuts.page:34(note/p)
msgid "Use the toolbar shortcut if your keyboard does not have a <key>Pause</key> key."
msgstr "Uporabite bližnjico orodne vrstice, če vaša tipkovnica nima tipke <key>Pause</key>."

#: C/shortcuts.page:37(td/p)
msgid "Help"
msgstr "Pomoč"

